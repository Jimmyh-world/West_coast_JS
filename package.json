{
  "name": "west_coast_js",
  "version": "1.0.0",
  "description": "Individual assignment for front-end Javascript course",
  "main": "src/js/main.js",
  "type": "module",
  "scripts": {
    "start": "json-server --watch data/db.json",
    "clean": "rm -rf dist/",
    "build": "npm run clean && tsc",
    "dev": "concurrently \"npm run watch\" \"live-server --config=live-server.config.js\"",
    "watch": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "tsc": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jimmyh-world/West_coast_JS.git"
  },
  "keywords": [],
  "author": "",
