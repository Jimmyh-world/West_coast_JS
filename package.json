  "bugs": {
    "url": "https://github.com/Jimmyh-world/West_coast_JS/issues"
  },
  "homepage": "https://github.com/Jimmyh-world/West_coast_JS#readme",
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/json-server": "^0.14.7",
    "@types/live-server": "^1.2.3",
    "@types/node": "^22.13.1",
    "concurrently": "^9.1.2",
    "jsdom": "^26.0.0",
    "json-server": "^1.0.0-beta.3",
    "live-server": "^1.2.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
